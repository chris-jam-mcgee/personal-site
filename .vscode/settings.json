{
    "editor.tabSize": 4,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true
    },
    "[yaml]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.tabSize": 2
    },
    "[toml]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.tabSize": 2
    },
    "[markdown]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.tabSize": 2
    },
    "typescript.tsdk": "node_modules/typescript/lib",
    "files.associations": {
        "*.css": "tailwindcss",
    },
    "cSpell.words": [
        "bandaids",
        "Dbeaver",
        "FEMAP",
        "frangibolts",
        "Gantt",
        "Hardwire",
        "JHUAPL",
        "kapton",
        "lockin",
        "makerspace",
        "mechatronics",
        "Microcenter",
        "microcontroller",
        "microcontrollers",
        "multibillion",
        "nitinol",
        "odometry",
        "offgass",
        "offgassing",
        "photoresister",
        "pyinstaller",
        "Pytest",
        "RFID",
        "roadmaps",
        "Simulink",
        "slackbot",
        "Solidworks",
        "STIGs",
        "Tailscale",
        "TQCM",
        "Unclonable"
    ]
}
